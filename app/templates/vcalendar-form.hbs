<div class="well">
	<form class="form-horizontal" role="form">
		{{input x-wr-calname hintTranslation="calendar.form.x-wr-calname.hint" labelTranslation="calendar.form.x-wr-calname.label"}}
		<br />
		{{input x-wr-caldesc hintTranslation="calendar.form.x-wr-caldesc.hint" labelTranslation="calendar.form.x-wr-caldesc.label"}}
		<br />
		{{input x-wr-timezone hintTranslation="calendar.form.x-wr-timezone.hint" labelTranslation="calendar.form.x-wr-timezone.label"}}
		<br />
		<div class="hide">
			{{input prodid hintTranslation="calendar.form.prodid.hint" labelTranslation="calendar.form.prodid.label"}}
			<br />
			{{input version hintTranslation="calendar.form.version.hint" labelTranslation="calendar.form.version.label"}}
			<br />
		</div>

		<div class="form-group">
			<div class="col-sm-offset-1 col-sm-10">
				<br />
				{{#bs-button type="primary" large=true block=true clicked="submit" clickedParam="this"}}{{t calendar.form.save}}{{/bs-button}}
				{{!--{{submit}}--}}
			</div>
		</div>
	</form>
	<div class="clearfix"></div>
</div>
